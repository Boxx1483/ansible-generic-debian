# apt-conf

{# add proxy if applicable #}
{% if apt_proxy_url %}
Acquire::http::Proxy "{{ apt_proxy_url }}";
{% else %}
#Acquire::http::Proxy "http://someserver:9999";
{% endif %}

{# set the default release #}
{% if apt_default_release is defined %}
APT::Default-Release "{{ apt_default_release }}";
{% endif %}

